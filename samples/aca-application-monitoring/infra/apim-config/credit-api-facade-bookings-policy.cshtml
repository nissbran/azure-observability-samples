<policies>
    <inbound>
        <base />
        <set-backend-service backend-id="booking-api-aca-backend" />
        <rewrite-uri template="/bookings/{id}" copy-unmatched-params="false" />
        <set-query-parameter name="api-version" exists-action="override">
            <value>2024-10-30</value>
        </set-query-parameter>
    </inbound>
    <backend>
        <base />
    </backend>
    <outbound>
        <base />
    </outbound>
    <on-error>
        <base />
    </on-error>
</policies>